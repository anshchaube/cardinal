[OCCA]
backend = CPU
deviceNumber = LOCAL-RANK

[GENERAL]
polynomialOrder = 4
dt = 5e-04
writeInterval = 10000
numSteps = 100
extrapolation = OIFS
targetCFL = 2.0

[PROBLEMTYPE]
equation = incompNS
stressFormulation = true # required for variable viscosity

[PRESSURE]
residualTol = 1e-6

[VELOCITY]
boundaryTypeMap = v, O, W 
residualTol = 1e-8
density   = 1.0
viscosity = 1e-04

[TEMPERATURE]
solver = none
boundaryTypeMap = t, I, I

[GENERAL]
  startFrom = "restart.fld"
  polynomialOrder = 7
#  stopAt = endTime #numSteps
#  endTime = 600
  numSteps = 100
  dt = 1e-3
#  dt = targetCFL=0.45 + max=1e-1 + initial=1e-3
  timeStepper = tombo2
  constFlowRate = meanVelocity=1.0 + direction=X
  writeInterval = 10

[MESH]
  file = "turbPipe.re2"

[PRESSURE]
  residualTol = 1e-6

[VELOCITY]
  density = 1.0
  viscosity = -100.0
  boundaryTypeMap = wall
  residualTol = 1e-8

[TEMPERATURE]
  solver = none

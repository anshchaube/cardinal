c-----------------------------------------------------------------------
      subroutine usrdat2
      implicit none
      include 'SIZE'
      include 'TOTAL'

      integer iel, ifc, i, ntot
      real theta, xx, zz

      do iel=1,nelt
      do ifc=1,2*ndim
         if (cbc(ifc,iel,1) .eq. 'W  ') boundaryID(ifc,iel) = 1
      enddo
      enddo

      theta = pi/2.0
      ntot = lx1*ly1*lz1*nelv
      do i =  1, ntot
        xx = xm1(i,1,1,1)
        zz = zm1(i,1,1,1)
        xm1(i,1,1,1) = xx*cos(theta) - zz*sin(theta) + 6.0
        zm1(i,1,1,1) = xx*sin(theta) + zz*cos(theta)
      enddo

      return
      end

[OCCA]
  backend = CPU
  deviceNumber = 0

[GENERAL]
  stopAt = numSteps
  numSteps = 1000
  dt = 5.0e-4
  polynomialOrder = 3
  writeControl = timeStep
  writeInterval = 2
  extrapolation = subCycling

[VELOCITY]
  viscosity = 1.0
  density = 1.0
  residualTol = 1.0e-6
  residualProj = no
  boundaryTypeMap = symz, symz, inlet, outlet, symy, symy

[PRESSURE]
  residualTol = 1.0e-5
  residualProj = no

[TEMPERATURE]
  solver = none
  residualTol = 1e-5

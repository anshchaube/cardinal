[OCCA]
  backend = CPU
  deviceNumber = 0

[GENERAL]
  stopAt = numSteps
  numSteps = 10
  dt = 0.1
  polynomialOrder = 7
  writeControl = timeStep
  writeInterval = 10
  extrapolation = subCycling

[VELOCITY]
  solver = none
  residualTol = 1.0e-6
  residualProj = no
  boundaryTypeMap = inlet, outlet, wall, wall, wall, wall

[PRESSURE]
  residualTol = 1.0e-5
  residualProj = no

[TEMPERATURE]
  conductivity = 1.5
  rhoCp = 1.0
  residualTol = 1.0e-5
  residualProj = no
  boundaryTypeMap = I, t, t, t, t, I

[OCCA]
  backend = CPU
  deviceNumber = 0

[GENERAL]
  stopAt = numSteps
  numSteps = 10
  dt = 0.1
  polynomialOrder = 4
  writeControl = timeStep
  writeInterval = 50
  extrapolation = subCycling

[VELOCITY]
  solver = none
  residualTol = 1.0e-6
  residualProj = no
  boundaryTypeMap = wall, wall, inlet, outlet

[PRESSURE]
  residualTol = 1.0e-5
  residualProj = no

[TEMPERATURE]
  conductivity = 2.5
  rhoCp = 1.0
  residualTol = 1.0e-10
  residualProj = no
  boundaryTypeMap = t, f, I, I

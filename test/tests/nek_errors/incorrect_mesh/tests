[Tests]
  [./incorrect_mesh]
    type = RunException
    input = nek.i

    # nekRS can't use more processors than elements
    max_parallel = 12

    expect_err = "Mesh for a 'NekRSProblem' must be of type 'NekRSMesh'!"
    requirement = "The system shall error if NekRSProblem is not paired with the correct mesh type."
  [../]

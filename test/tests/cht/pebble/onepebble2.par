[OCCA]
backend = CPU
deviceNumber = 0

[GENERAL]
polynomialOrder = 5
numSteps = 5
dt = 1e-04
writeInterval = 5
extrapolation = subCycling

[PRESSURE]
preconditioner = semg_amg
residualTol = 1e-04
residualProj = no

[VELOCITY]
boundaryTypeMap = W, O, v
residualTol = 1e-04
density = 1.0
viscosity = 0.00005

[TEMPERATURE]
boundaryTypeMap = f, I, t
rhoCp = 100000.0
conductivity = 5.0
residualTol = 1e-03
residualProj = yes
